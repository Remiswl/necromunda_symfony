{% extends 'base.html.twig' %}

{% block title %}New {{ topic }}{% endblock %}

{% block body %}

	<section id="new_{{ topic | lower }}" class="mb-5">

		{% set previousPage = path('settings') %}
		{% include 'back.html.twig' %}

		<div class="col-sm-8 offset-lg-4 col-lg-4 m-auto text-white">
			{% include 'settings/new' ~ topic ~ '.html.twig' %}
		</div>
	</section>

{% endblock %}