{% extends 'base.html.twig' %}

{% block title %}Gangs{% endblock %}

{% block body %}

<div class="text-center text-white">
	<p>List of gangs</p>

	<ul>
		{% for user in users %}
			<li><a href="{{ path('gangs', {id : user.id}) }}">{{ user.pseudo }} - {{ user.gangName }} - {{ user.gangTypeId }}</a></li>
		{% endfor %}
	</ul>
</div>

{% endblock %}
