{% extends 'base.html.twig' %}

{% block title %}Edit {{ topic }}{% endblock %}

{% block body %}

	<section id="edit_{{ topic | lower }}" class="mb-5">

		{% set previousPage = path('settings') %}
		{% include 'back.html.twig' %}

		<div class="col-sm-8 offset-lg-4 col-lg-4 m-auto text-white">
			{% include 'settings/edit' ~ topic ~ '.html.twig' %}
		</div>
	</section>

{% endblock %}